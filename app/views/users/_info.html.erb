<h1>User info</h1>
  <%= image_tag user.get_profile_image(100,100) %>
  <table>
    <tbody>
      <tr>
        <th>name</th>
        <th><%= user.name %></th>
      </tr>
      <tr>
        <th>introduction</th>
        <th><%= user.introduction %></th>
      </tr>
    </tbody>
  </table>

<%= link_to edit_user_path(user), class: "btn btn-outline-secondary" do %>
  <i class="fa-solid fa-user-gear"></i>
<% end %>
